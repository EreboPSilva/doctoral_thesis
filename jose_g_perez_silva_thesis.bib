file = {:Users/erebo/Dropbox/perez-silva/l{\_}space/papers/Kehlmaier et al.. Tropical ancient DNA reveals relationships of the extinct bahamian giant tortoise Chelonoidis alburyorum. 2017.pdf:pdf},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Bahamas,Biogeography,Extinction,Palaeontology,Phylogeny},
month = {jan},
number = {1846},
pages = {20162235},
pmid = {28077774},
publisher = {The Royal Society},
title = {{Tropical ancient DNA reveals relationships of the extinct bahamian giant tortoise Chelonoidis alburyorum}},
