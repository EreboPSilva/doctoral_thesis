pmid = {17393015},
title = {{Analysis of the novel factor X gene mutation Glu51Lys in two families with factor X-Riyadh anomaly.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17393015},
volume = {97},
year = {2007}
}
