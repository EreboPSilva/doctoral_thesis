journal = {Neurobiology of Aging},
keywords = {Chinese,Early-onset familial Alzheimer's disease,Mutation,PSEN1},
number = {3},
pages = {1602.e3--1602.e6},
title = {{Identification of PSEN1 mutations p.M233L and p.R352C in Han Chinese families with early-onset familial Alzheimer's disease}},
volume = {36},
year = {2015}
}
