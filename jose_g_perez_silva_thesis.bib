isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {48},
pages = {17230--17235},
pmid = {25385592},
title = {{Comparative analysis of the domestic cat genome reveals genetic signatures underlying feline biology and domestication}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1410083111},
volume = {111},
