doi = {10.1038/s41586-020-2012-7},
file = {:Users/erebo/Dropbox/perez-silva/l{\_}space/papers/Zhou et al.. A pneumonia outbreak associated with a new coronavirus of probable bat origin. 2020.pdf:pdf},
