isbn = {0888-7543},
issn = {08887543},
journal = {Genomics},
keywords = {Genome,Japanese quail,Microsatellite,Next-generation sequencing technology},
number = {6},
pages = {345--353},
pmid = {23557672},
publisher = {Elsevier Inc.},
title = {{Next-generation sequencing reveals genomic features in the Japanese quail}},
