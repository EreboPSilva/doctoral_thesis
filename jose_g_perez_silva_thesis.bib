file = {:Users/erebo/Dropbox/perez-silva/l{\_}space/papers/Mu{\~{n}}oz-Lorente, Cano-Martin, Blasco. Mice with hyper-long telomeres show less metabolic aging and longer lifespans. 2019.pdf:pdf},
issn = {20411723},
journal = {Nature Communications},
