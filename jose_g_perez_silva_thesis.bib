file = {:Users/erebo/Dropbox/perez-silva/l{\_}space/papers/Alf{\"{o}}ldi et al.. The genome of the green anole lizard and a comparative analysis with birds and mammals. 2011.pdf:pdf},
isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
