title = {{Intermittent Fasting Promotes White Adipose Browning and Decreases Obesity by Shaping the Gut Microbiota Intermittent Fasting Promotes White Adipose Browning and Decreases Obesity by Shaping the Gut Microbiota}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28918936{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5668683{\%}0Ahttp://dx.doi.org/10.1016/j.cmet.2017.08.019},
