file = {:Users/erebo/Dropbox/perez-silva/l{\_}space/papers/Chen et al.. Whole-genome sequence of a flatfish provides insights into ZW sex chromosome evolution and adaptation to a benthic lifestyl.pdf:pdf},
isbn = {1061-4036},
